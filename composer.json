{
    "name": "hitechcoding/strict-form-mapper-bundle",
    "type": "symfony-bundle",
    "description": "Improved data mapper for your forms",
    "keywords": [
        "form mapper"
    ],
    "license": "MIT",
    "require": {
        "php": "~7.1",
        "ext-json": "*",
        "doctrine/annotations": "^1.6",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/contracts": "^1.1",
        "symfony/dependency-injection": "^3.4|^4.1",
        "symfony/form": "^3.4|^4.1",
        "symfony/framework-bundle": "^3.4|^4.1",
        "symfony/options-resolver": "^3.4|^4.1",
        "symfony/polyfill-php73": "^1.12",
        "symfony/translation": "^3.4|^4.1",
        "symfony/twig-bundle": "^3.4|^4.1",
        "symfony/validator": "^3.4|^4.1",
        "symfony/yaml": "^3.4|^4.1"
    },
    "require-dev": {
        "phpstan/phpstan-shim": "^0.11.5",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^3.4|^4.1",
        "symfony/css-selector": "^3.4|^4.1",
        "symfony/phpunit-bridge": "^3.4|^4.1",
        "symfony/var-dumper": "^3.4|^4.1"
    },
    "autoload": {
        "psr-4": {
            "HTC\\StrictFormMapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HTC\\StrictFormMapper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
